           <script>
             $(document).ready(function() {
               $("#filieres").change(function(){
                 $.ajaxSetup({
                      headers: {
                          'X-CSRF-TOKEN': $('meta[name="csrf_token"]').attr('content')
                      }
                  });
                  var id=$("#filieres").val();
                  $.ajax({  //create an ajax request to display.php
                    type: "POST",
                    url: "/getModules",   
                    data:{id_filiere:id, _token:'hSdpp1IfMHiPNpd1MZnMaYIBLvTlgk0CUxWwKtq7'},
                    success: function (data) {
                      result = "";
                      console.log(data);
                      document.getElementById('matieres').innerHTML = "";
                      
                      jQuery.each(data, function() {
                        var content = this.module + " ( " + this.libelle + " )";
                        document.getElementById('matieres').options.add(new Option(content, this.id));
                      });
                      document.getElementById('matieres').options.add(new Option('Tous les modules & matieres',0));
                      }
                    });
                  
                }); document.getElementById('refresh').onclick = function(){
                      $.ajaxSetup({
                          headers: {
                              'X-CSRF-TOKEN': $('meta[name="csrf_token"]').attr('content')
                          }
                      });
                      var month = id=$("#month").val(),
                          year = id=$("#year").val(),
                          id_filiere = id=$("#filieres").val(),
                          id_matiere = id=$("#matieres").val();
                          
                      $.ajax({
                          type: "POST",
                          url: "/getinfo",
                          data: { month: month,
                                  year: year,
                                  id_filiere: id_filiere,
                                  id_matiere: id_matiere,
                                  _token:'hSdpp1IfMHiPNpd1MZnMaYIBLvTlgk0CUxWwKtq7'},
                          success: function(data){
                            console.log(data);
                            var tbody = document.getElementById('tbody'),
                                thead = document.getElementById('thead');
                            tbody.innerHTML = "";
                            thead.innerHTML = "";
                            var t = "",
                                h = "",
                                i = 0;
                            number_days = new Date(year, month, 0).getDate();
                            h += "<th rowspan=2 style='padding: 28px;''>CNE</th>";
                            h += "<th rowspan=2 style='padding: 28px;'>Nom</th>";
                            h += "<th rowspan=2 style='padding: 28px;'>Prénom</th>";
                            h += "<th rowspan=2 style='padding: 28px;'>Absences</th>";
                            
                            for(i= 1;i<=number_days;i++){
                              h += "<th colspan='4' style='text-align: center;'>"+i+"</th>";
                            }
                            h += "</tr>";
                            h += "<tr class='border-top: 1px solid #ededed'>";
                            for (var i = 1; i <= number_days; i++){
                              h += "<th style='text-align: center;'> 09h </th>";
                              h += "<th style='text-align: center;'> 11h </th>";
                              h += "<th style='text-align: center;'> 14h </th>";
                              h += "<th style='text-align: center; border :1px solid #555'> 16h </th>";
                            }
                            h +="</tr>";

                            thead.innerHTML = h;
                            jQuery.each(data, function() {
                              t += "<tr class='gradeU' style='border-bottom:1.1px solid #aaa'>";
                              t +=  "<td>"+this.cne+"</td>";
                              t +=  "<td>"+this.nom+"</td>";
                              t +=  "<td>"+this.prenom+"</td>";
                              t +=  "<td style='text-align:center; color: rgb(155, 111, 111); font-weight: bold'>"+(this.absences).length+"</td>";
                              
                                
                                for(day=1; day<=number_days; day++){
                                  if(day<10)
                                    jour = year+'-'+month+'-0'+day;
                                  else
                                    jour = year+'-'+month+'-'+day;

                                  jourAnd9 = jour+' '+'09';
                                  jourAnd11 = jour+' '+'11';
                                  jourAnd14 = jour+' '+'14';
                                  jourAnd16 = jour+' '+'16';

                                  count_by_day_and_9 = 0;
                                  count_by_day_and_11 = 0;
                                  count_by_day_and_14 = 0;
                                  count_by_day_and_16 = 0;

                                  justifie9 = justifie11 = justifie14 = justifie16 = 1;
                                  absences = this.absences;
                                  if(absences.length){
                                    jQuery.each(absences, function(){
                                      if(this.date_heure.startsWith(jourAnd9)){
                                        count_by_day_and_9++;
                                        justifie9 = this.justifie;
                                      }
                                      if(this.date_heure.startsWith(jourAnd11)){
                                        count_by_day_and_11++;
                                        justifie11 = this.justifie;
                                      }
                                      if(this.date_heure.startsWith(jourAnd14)){
                                        count_by_day_and_14++;
                                        justifie14 = this.justifie;
                                      }
                                      if(this.date_heure.startsWith(jourAnd16)){
                                        count_by_day_and_16++;
                                        justifie16 = this.justifie;
                                      }
                                    });
                                  }
                                  if(justifie9){
                                    color9 = "rgb(71, 163, 133)";
                                  }else{
                                    color9 = "rgb(155, 111, 111)";
                                  }
                                  if(justifie11){
                                    color11 = "rgb(71, 163, 133)";
                                  }else{
                                    color11 = "rgb(155, 111, 111)";
                                  }
                                  if(justifie14){
                                    color14 = "rgb(71, 163, 133)";
                                  }else{
                                    color14 = "rgb(155, 111, 111)";
                                  }
                                  if(justifie16){
                                    color16 = "rgb(71, 163, 133)";
                                  }else{
                                    color16 = "rgb(155, 111, 111)";
                                  }
                                  if(count_by_day_and_9){
                                    t += "<td style='text-align:center; color: "+color9+"; font-weight: bold; border-left: 1px solid #555'>X</td>";
                                  }else{
                                    t += "<td style='border-left: 1px solid #555'> </td>";
                                  }

                                  if(count_by_day_and_11){
                                    t += "<td style='text-align:center; color: "+color11+"; font-weight: bold'>X</td>";
                                  }else{
                                    t += "<td> </td>";
                                  }

                                  if(count_by_day_and_14){
                                    t += "<td style='text-align:center; color: "+color14+"; font-weight: bold'>X</td>";
                                  }else{
                                    t += "<td> </td>";
                                  }

                                  if(count_by_day_and_16){
                                    t += "<td style='text-align:center; color: "+color16+"; font-weight: bold; border-right: 1px solid #555'>X</td>";
                                  }else{
                                    t += "<td style='border-right: 1px solid #555'> </td>";
                                  }
                                }
                                t +=  "</tr>";
                            });
                            tbody.innerHTML = t;
                            console.log(data);
                            
                          }
                      });
                    };
              });
           </script>
          </div>
